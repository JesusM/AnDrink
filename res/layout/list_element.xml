<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content" android:background="@color/lightgray"
	android:orientation='horizontal'>
	<LinearLayout android:layout_width='fill_parent' android:id="@+id/list_element_layout"
		android:layout_marginTop="2dip" android:layout_marginBottom="2dip" 
		android:layout_height='fill_parent' android:orientation='horizontal'
		android:layout_weight='1.0' android:layout_gravity="center">
		<TextView android:layout_width='fill_parent' android:id="@+id/textviewlistelement1"
			android:textColor="@color/black" android:layout_height='fill_parent'
			android:gravity='center' android:layout_weight="1.0"></TextView>
		<TextView android:layout_width='fill_parent' android:id="@+id/textviewlistelement2"
			android:textColor="@color/black" android:layout_height='fill_parent'
			android:gravity='center' android:layout_weight="1.0"></TextView>
		<TextView android:layout_width='fill_parent' android:id="@+id/textviewlistelement3"
			android:textColor="@color/black" android:layout_height='fill_parent'
			android:gravity='center' android:layout_weight="1.0"></TextView>
		<TextView android:layout_width='fill_parent' android:id="@+id/textviewlistelement4"
			android:textColor="@color/black" android:layout_height='fill_parent'
			android:gravity='center' android:layout_weight="1.0"></TextView>
		<TextView android:layout_width='fill_parent' android:id="@+id/textviewlistelement5"
			android:textColor="@color/black" android:layout_height='fill_parent'
			android:gravity='center' android:layout_weight="1.0"></TextView>
	</LinearLayout>


</LinearLayout>
